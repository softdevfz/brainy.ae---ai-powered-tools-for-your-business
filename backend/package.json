{
  "name": "brainy-ae-backend",
  "version": "1.0.0",
  "description": "Backend service for Brainy.ae payment processing and subscription management",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "knex": "^3.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "google-auth-library": "^9.0.0",
    "paypal-rest-sdk": "^1.8.1",
    "node-cron": "^3.0.2",
    "winston": "^3.10.0",
    "joi": "^17.9.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.14",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcryptjs": "^2.4.4",
    "@types/node": "^20.5.0",
    "@types/uuid": "^9.0.3",
    "typescript": "^5.1.6",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
} 